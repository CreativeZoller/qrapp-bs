{
  "name": "qrapp-bs",
  "title": "QR code generator application",
  "version": "0.0.41",
  "description": "This is an open-source web-application to generate custom QR codes and download them as images",
  "keywords": [
    "qr",
    "qr code",
    "generator"
  ],
  "private": false,
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/CreativeZoller/qrapp-bs.git"
  },
  "author": {
    "name": "Zoltan Belicza",
    "email": "hello@zoltanbelicza.eu",
    "url": "https://zoltanbelicza.eu/"
  },
  "bugs": {
    "url": "https://github.com/CreativeZoller/qrapp-bs/issues"
  },
  "homepage": "https://github.com/CreativeZoller/qrapp-bs#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --detect-open-handles",
    "lint": "prettier --check .",
    "e2e": "ng e2e",
    "postinstall": "npm prune",
    "start-pwa": "ng build --prod && http-server -p 8080 -c-1 dist/qrapp-bs",
    "predeploy": "ng build --prod --base-href=/qrapp-bs/",
    "deploy": "npx gh-pages -d dist/qrapp-bs"
  },
  "dependencies": {
    "@angular/animations": "~11.2.3",
    "@angular/common": "~11.2.3",
    "@angular/compiler": "~11.2.3",
    "@angular/core": "~11.2.3",
    "@angular/forms": "~11.2.3",
    "@angular/platform-browser": "~11.2.3",
    "@angular/platform-browser-dynamic": "~11.2.3",
    "@angular/router": "~11.2.3",
    "@angular/service-worker": "~11.2.3",
    "@briebug/jest-schematic": "^3.1.0",
    "bootstrap": "^4.6.0",
    "easyqrcodejs": "^4.3.2",
    "http-server": "^0.12.3",
    "jquery": "^3.5.1",
    "popper": "^1.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "11.1.0",
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.3",
    "@types/bootstrap": "^5.0.8",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^26.0.20",
    "@types/jquery": "^3.5.5",
    "@types/luxon": "^1.26.2",
    "@types/node": "^12.20.4",
    "codelyzer": "^6.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.4.0",
    "karma-coverage": "~2.0.3",
    "lint-staged": "8.1.7",
    "luxon": "^1.26.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-jest": "^26.5.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
